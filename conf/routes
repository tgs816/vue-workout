GET         /                              controllers.Home.index

GET         /                              controllers.Home.home


GET         /currency                      controllers.Home.currency

GET         /currencyAdd                   controllers.Home.currencyAddPage
POST        /currencyAdd                   controllers.Home.currencyAdd

GET         /currency/:id                  controllers.Home.currencyEditPage(id: String)
POST        /currency/Edit                 controllers.Home.currencyEdit

GET         /currency/Delete/:id           controllers.Home.currencyDelete(id: String)

GET         /assets/*file                  controllers.Assets.at(path="/public", file)